<!--定义Mapper映射文件的约束和基本结构-->
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.franklin.mapper.LoginMapper">

    <select id="login" resultType="com.franklin.dto.LoginResponseDTO">
        select id, user_name as username, name from emp
        where user_name = #{username} and password = #{password}
    </select>
</mapper>